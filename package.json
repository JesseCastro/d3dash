{
  "name": "d3dash",
  "version": "1.0.0",
  "description": "D3 dashboard library",
  "main": "index.html",
  "scripts": {
    "ci": "npm run lint && npm test",
    "test": "npm run test:karma",
    "test:karma": "karma start",
    "test:karma:watch": "karma start --singleRun false",
    "lint": "jshint src && jscs src",
    "hooks": "cp ./hooks .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "bump": "mversion ",
    "build": "webpack ",
    "dist": "webpack --output-path dist ",
    "dist:minimize": "webpack --output-path dist -p --output-filename rackspace-js-lib.min.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "docs": "jsdoc -c jsdoc.conf.json ./src",
    "preversion": "git rm -r dist docs && npm run dist && npm run changelog && npm run docs && git add dist docs CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JesseCastro/d3dash.git"
  },
  "keywords": [
    "d3",
    "dashboard"
  ],
  "author": "Jesse R. Castro <jesse.r.castro@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JesseCastro/d3dash/issues"
  },
  "homepage": "https://github.com/JesseCastro/d3dash#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^1.1.0",
    "css-loader": "^0.23.1",
    "ejs-loader": "^0.2.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "ink-docstrap": "^1.1.0",
    "jscs": "^2.7.0",
    "jsdoc": "^3.4.0",
    "jshint": "^2.8.0",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.6.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-solarized": "^1.0.2",
    "c3": "^0.4.11",
    "datamaps": "^0.5.8",
    "jquery": "^3.1.1",
    "lodash": "^3.10.1",
    "topojson": "^1.6.27"
  }
}
